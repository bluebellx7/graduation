<view class="container"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">登录</view></cu-custom><image style="height:190rpx;" src="../../static/image/1.jpg"></image><view class="{{['cu-card case',isCard?'no-card':'']}}" style="align-items:center;margin-top:0rpx;"><view class="cu-item shadow"><view class="cu-form-group"><input style="font-size:32rpx;" placeholder="请输入账号" name="input" data-event-opts="{{[['input',[['__set_model',['','userId','$event',[]]]]]]}}" value="{{userId}}" bindinput="__e"/></view><view class="cu-form-group"><input style="font-size:32rpx;" type="password" placeholder="请输入密码" name="input" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="cu-btn bg-blue lg round" bindtap="__e">登录</button></view><view style="display:flex;justify-content:space-between;"><view data-event-opts="{{[['tap',[['forget',['$event']]]]]}}" style="color:#888888;font-size:30rpx;margin-left:40rpx;margin-top:16rpx;" bindtap="__e">忘记密码</view><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" style="color:#888888;font-size:30rpx;margin-right:40rpx;margin-top:16rpx;" bindtap="__e">注册账户</view></view></view></view>